{
  "name": "mv-wollbach",
  "license": "MIT",
  "version": "21.01.26",
  "description": "Hugo theme for the website of Musikverein Wollbach 1866 e.V.",
  "author": "Markus Lindenmann <tiliavir@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tiliavir/mv-website.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "ie 9-11"
  ],
  "devDependencies": {
    "@types/jquery": "3.5.5",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "autoprefixer": "10.2.3",
    "copyfiles": "2.4.1",
    "eslint": "7.18.0",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "sass-lint": "1.13.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "jquery": "3.5.1"
  },
  "scripts": {
    "lint": "sass-lint -c sass-lint.yml -v -q && npx eslint . --ext .ts",
    "update:lib:jquery": "copyfiles -u 3 ./node_modules/jquery/dist/jquery.min.js ./static/js/",
    "postinstall": "npm run update:lib:jquery && npm run lint"
  }
}
