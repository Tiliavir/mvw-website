    var title = "Jahreskonzert 2016";
    var description = "Still Feeling Good - Das Jahreskonzert des Musikverein Wollbach im Jahr 2016 anlässlich des 150 jährigen Jubiläums.";
    var keywords = "Jahreskonzert, Jubiläum, 150 Jahre, Still Feeling Good, Jahreskonzert 2016, Konzert";
    var publishedDate = "2015-11-01T00:00:00.000Z";
    var ampLogoUrl = "files/flyer/16_jahreskonzert.jpg";
