{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Register",
  "description": "Schema for members.",

  "type": "array",

  "items": {
    "type": "object",

    "properties": {
      "name": {
        "type": "string"
      },
      "familyName": {
        "type": "string"
      },
      "register": {
        "enum": [ "Querflöte", "Oboe", "Klarinette", "Fagott", "Altsaxophon", "Tenorsaxophon", "Baritonsaxophon", "Trompete", "Horn", "Tenorhorn", "Posaune", "Euphonium", "Tuba", "Schlagzeug", "Klavier", "Gitarre" ],
        "description": "All available registers."
      }
    },
    "additionalProperties": false,
    "required": [ "name", "familyName", "register" ]
  },
  "additionalItems": false
}
